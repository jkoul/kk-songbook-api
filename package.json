{
  "name": "kk-songbook-api",
  "description": "Apollo GraphQL API server for the Kostume Karaoke songbook project",
  "version": "0.0.0",
  "main": "src",
  "author": {
    "name": "Jeremy Koulish"
  },
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^10.13.0",
    "npm": "6.5.0"
  },
  "scripts": {
    "start": "node index",
    "dev": "nodemon index",
    "clean": "rimraf dist",
    "test": "jest --watch",
    "lint": "eslint src/. --config .eslintrc.json"
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "bull": "^3.4.8",
    "codeship-git-rev-sync": "0.0.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "dotenv-safe": "^6.1.0",
    "express": "^4.16.4",
    "graphql": "^0.13.1",
    "graphql-server-express": "^1.3.2",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.21.0",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "mongodb": "^3.1.10",
    "mongojs": "^2.6.0",
    "mongoose": "^5.4.0",
    "passport-jwt": "^4.0.0",
    "passport-localapikey": "0.0.3",
    "serve-favicon": "^2.4.5",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "casual": "^1.5.19",
    "eslint": "^5.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.17.5",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "supertest": "^3.1.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "scripts"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jestSetup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.js",
      "!src/components/themes/*.js",
      "!src/store/{configure,reducer,sagas}.js",
      "!<rootDir>/node_modules"
    ]
  }
}
